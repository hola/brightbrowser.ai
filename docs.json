{
    "$schema": "https://mintlify.com/docs.json",
    "name": "Browser.ai Documentation",
    "theme": "maple",
    "contextual": {
        "options": [
            "copy",
            "view",
            "chatgpt",
            "claude",
            "perplexity",
            "mcp",
            "cursor",
            "vscode"
        ]
    },
    "colors": {
        "primary": "#A1DE02",
        "light": "#CDFD50",
        "dark": "#FFE7E6"
    },
    "fonts": {
        "heading": {
            "family": "Inter"
        },
        "body": {
            "family": "Inter"
        }
    },
    "background": {
        "image": {
            "light": "/logo/light3.svg",
            "dark": "/logo/dark.svg"
        }
    },
    "search": {
        "prompt": "Type a keyword to find setup guides..."
    },
    "favicon": "/favicon24.svg",
    "navigation": {
        "tabs": [
            {
                "tab": "Documentation",
                "groups": [
                    {
                        "group": "Welcome",
                        "pages": [
                            "general/intro",
                            "general/start",
                            "general/faqs",
                            "general/troubleshooting"
                        ]
                    },
                    {
                        "group": "API",
                        "pages": [
                            "api-token/api-intro",
                            "api-token/api-geolocation",
                            "api-token/crawler-automation",
                            "api-token/script-automation"
                        ]
                    },
                    {
                        "group": "Headless Browser",
                        "pages": [
                            "headless-browser/log-in",
                            "headless-browser/captcha-solver",
                            "headless-browser/geolocation",
                            "general/configuration",
                            "headless-browser/cdp-func",
                            "general/code-snippets"
                        ]
                    }
                ]
            },
            {
                "tab": "API Reference",
                "groups": [
                    {
                        "group": "API Overview",
                        "pages": [
                            "api-reference/intro",
                            "api-reference/token",
                            "api-reference/n8n-node"
                        ]
                    },
                    {
                        "group": "API Endpoints",
                        "pages": [
                            "api-reference/endpoint/tasks",
                            "api-reference/endpoint/forceStop",
                            "api-reference/endpoint/addInstructions",
                            "api-reference/endpoint/updateInstructions",
                            "api-reference/endpoint/taskMetadata",
                            "api-reference/endpoint/results",
                            "api-reference/endpoint/createSavedTask",
                            "api-reference/endpoint/getSavedTaskList",
                            "api-reference/endpoint/getSavedTaskName"
                        ]
                    }
                ]
            }
        ]
    },
    "logo": {
        "dark": "/logo/color=light, size=m.svg",
        "light": "/logo/color=mix, size=m.svg",
        "href": "https://browser.ai/"
    },
    "navbar": {
        "links": [
            {
                "label": "Support",
                "href": "mailto:hello@browser.ai"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Dashboard",
            "href": "https://browser.ai/"
        }
    },
    "footer": {
        "socials": {}
    }
}