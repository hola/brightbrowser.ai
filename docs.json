{
    "$schema": "https://mintlify.com/docs.json",
    "name": "Browser.ai Documentation",
    "theme": "maple",
    "colors": {
        "primary": "#A1DE02",
        "light": "#CDFD50",
        "dark": "#FFE7E6"
    },
    "fonts": {
        "heading": {
            "family": "Inter"
        },
        "body": {
            "family": "Inter"
        }
    },
    "background": {
        "image": {
            "light": "/logo/light3.svg",
            "dark": "/logo/dark.svg"
        }
    },
    "search": {
        "prompt": "Type a keyword to find setup guides..."
    },
    "favicon": "/logo/color=brand.png",
    "navigation": {
        "tabs": [
            {
                "tab": "Documentation",
                "groups": [
                    {
                        "group": "Welcome",
                        "pages": [
                            "general/intro",
                            "general/start",
                            "general/faqs"
                        ]
                    },
                  {
                        "group": "API",
                        "pages": [
                            "general/api-intro"
                        ]
                    },
                    {
                        "group": "Headless Browser",
                        "pages": [
                            "features/captcha-solver",
                            "features/geolocation",
                            "general/configuration",
                            "cdp-func/standard",
                            "general/code-snippets"
                        ]
                    }
                ]
            },
            {
                "tab": "API Reference",
                "groups": [
                    {
                        "group": "API Overview",
                        "pages": [
                            "api-reference/intro",
                            "api-reference/token"
                        ]
                    },
                    {
                        "group": "API Endpoints",
                        "pages": [
                            "api-reference/endpoint/tasks",
                            "api-reference/endpoint/taskMetadata",
                            "api-reference/endpoint/results"
                        ]
                    }
                ]
            }
        ]
    },
    "logo": {
        "dark": "/logo/color=brand.png",
        "light": "/logo/color=brand.png",
        "href": "https://browser.ai/"
    },
    "navbar": {
        "links": [
            {
                "label": "Support",
                "href": "mailto:hi@mintlify.com"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Dashboard",
            "href": "https://browser.ai/"
        }
    },
    "footer": {
        "socials": {}
    }
}